{
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "date": {
              "boost": 0.1,
              "format": "yyyy.MM.dd'T'HH:mm:ssZ",
              "gt": "2000.01.01T01:01:01+0300",
              "gte": "2000.01.02T01:01:01+0300",
              "lt": "2000.01.03T01:01:01+0300",
              "lte": "2000.01.04T01:01:01+0300"
            }
          }
        }
      ]
    }
  }
}